<?xml version="1.0"?>
<!DOCTYPE document [
<!ENTITY aDef SYSTEM "defs.xml">
]>
<xml-bindings
    xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
    
    package-name="jaxb.moxy.models">
    <java-types>
        <java-type name="App">
            <xml-root-element name="APP"/>
            <java-attributes>
                <xml-element java-attribute="module" name="MODULE"/>                
            </java-attributes>
        </java-type>
        
        
        <java-type name="Module">
          <xml-root-element name="MODULE"/>
          
          <java-attributes>
            <xml-element java-attribute="boofer" name="BOOFER"/>
          </java-attributes>
                    
        </java-type>
        
    </java-types>
</xml-bindings>
