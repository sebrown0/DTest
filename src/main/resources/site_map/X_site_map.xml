                <Element>
                  <Details type="Label" name="LabelGrade" text="Grade"/>
                  <Locator by="css" text="label[for='GRADE_CODE']"/>              
                </Element>
                <Element>
                  <Details type="Label" name="LabelStepLevel" text="Step Level"/>
                  <Locator by="css" text="label[for='STEP_LEVEL']"/>              
