<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<SiteMap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
  xsi:schemaLocation="Root SiteMap.xsd"
  xmlns="Root"
  xmlns:inf="SiteMapInfo"
  xmlns:inc="IncludeElementsInTest"
  xmlns:el="Element"
  xmlns:itm="MenuItem"
  xmlns:cnt="Container"
  xmlns:fun="Function"
  xmlns:din="TestDataIn"
  xmlns:dout="TestDataOut"
  xmlns:menus="Menus"
  xmlns:menu="Menu"
  xmlns:mod="Module"
  xmlns:mods="Modules"                
  >
  
  <Info>
    <inf:Author>SteveBrown</inf:Author>
    <inf:Version>1.0.0</inf:Version>
    <inf:CreationMethod>IgnoreExisting</inf:CreationMethod>
    <inf:RootDir>C:/Users/SteveBrown/eclipse-workspace/2021/DTest/src/main/java</inf:RootDir>
    <inf:ParentPackage>object_models/modules</inf:ParentPackage>
    <inf:ElementLibrary>C:/Users/SteveBrown/eclipse-workspace/2021/DTest</inf:ElementLibrary>
  </Info>
  
  <IncludeElementsForTest>
    <inc:Include>Button</inc:Include>
    <inc:Include>TextSelect</inc:Include>
    <inc:Include>TextOut</inc:Include>
    <inc:Include>ComboSelectOnly</inc:Include>    
    <inc:Include>Label</inc:Include>
  </IncludeElementsForTest>
  
  <Modules>
    <mods:Module name="payroll">
      <mod:Menus>        
        <menus:Menu name="Left" package="left_menu">        
          <menu:MenuItem name="Salary Details" package="Employees" class="SalaryDetails">
            <itm:Type name="JsPanelWithIFrame">
              <itm:JsPanelWithIFrame panelTitle="Employee Salary Details" /> 
            </itm:Type>            
            <itm:Container type="ItemGroup" name="HeaderElements">  
              <cnt:Container type="InputGroup" name="EmpLookup">
                <cnt:Locator by="css" text="div[class='input-group']"/>
                  <cnt:Element>
                    <el:Details type="TextOut" name="FormID" />
                    <el:Locator by="id" text="FORM_ID"/>   
                    <el:Function type="element" subtype="TextOut" isDefaultPass="true"/>
                    <!-- 
                    <el:TestDataIn insertWith="EmployeeLookupByName">                     
                      <din:Text value="Borg"/>
                    </el:TestDataIn>
                    <el:TestDataOut>                     
                      <dout:Text value="0123456M"/>
                    </el:TestDataOut>
                     -->                        
                  </cnt:Element>
                   
                <cnt:Element>
                  <el:Details type="Button" name="EmployeeList" text="" fafa="fa fa-list"/>
                  <el:Locator by="css" text="div[title='Search Employee']"/>  
                  <el:ToolTip text="Select from a list of employees within the chosen company"/>              
                  <el:Function type="element" subtype="TextOut" isDefaultPass="true" />
                </cnt:Element>                
                <cnt:Element>
                  <el:Details type="Button" name="SalaryHistory" text="" fafa="fa fa-list"/>
                  <el:Locator by="css" text="div[title='View Salary History']"/>
                  <el:ToolTip text="View Salary History"/>                
                  <el:Function type="element" subtype="Button" isDefaultPass="false"/>
                </cnt:Element>
                <cnt:Element>
                  <el:Details type="Button" name="Combos" text="" fafa="fa fa-window-maximize"/>
                  <el:Locator by="css" text="div[title='Combos']"/>
                  <el:ToolTip text="Combos"/>                
                  <el:Function type="element" subtype="Button" isDefaultPass="true" />
                </cnt:Element>
                <cnt:Element>
                  <el:Details type="Button" name="GridView" text="" fafa="fa fw fa-table"/>
                  <el:Locator by="css" text="div[title='Grid View for this Employee']"/>
                  <el:ToolTip text="Grid View for this Employee"/>                
                  <el:Function type="element" subtype="Button" isDefaultPass="true" />
                </cnt:Element>
                <cnt:Element>
                  <el:Details type="Button" name="ExistingRecords" text="" fafa="fa fw fa-table"/>
                  <el:Locator by="css" text="div[title='Grid View for existing records']"/>
                  <el:ToolTip text="Grid View for existing records"/>                
                  <el:Function type="element" subtype="Button" isDefaultPass="true" />
                </cnt:Element>
                <cnt:Element>
                  <el:Details type="Button" name="Documents" text="" fafa="fa fa-file-o"/>
                  <el:Locator by="css" text="div[title='No Documents Attached']"/>
                  <el:ToolTip text="No Documents Attached"/>                
                  <el:Function type="element" subtype="Button" isDefaultPass="true" />
                </cnt:Element>
              </cnt:Container>
              
              <cnt:Container type="InputGroup" name="DatePicker">
                <cnt:Locator by="css" text="div[class='input-group date datepicker']"/>                
                <cnt:Element>
                  <el:Details type="Button" name="Calendar" text="" fafa="fa fa-calendar fa-fw"/>
                  <el:Locator by="css" text="i[class='fa fa-calendar fa-fw']"/>         
                  <el:Function type="element" subtype="Button" isDefaultPass="true" />
                </cnt:Element>                                
              </cnt:Container>             
            </itm:Container>   
          
            <itm:Container type="ItemGroup" name="BodyElements">            
              <cnt:Container type="Tabs" name="Tabs">
                <cnt:Locator by="css" text="ul[class='nav nav-tabs']"/>
                <cnt:Container type="Tab" name="SalaryDetails">
                  <cnt:Locator by="css" text="a[id='tab-tab1']"/>
                  
                  <cnt:Element>
                    <el:Details type="Label" name="LabelReason" text="Reason"/>
                    <el:Locator by="css" text="label[for='REASON']"/>                   
                  </cnt:Element> 
                </cnt:Container>
                <cnt:Function type="container" isDefaultPass="true"/>     
              </cnt:Container>     
            </itm:Container>
            
          </menu:MenuItem>
         
          <menu:MenuItem name="Contact Numbers" package="Employees" class="ContactNumbers">
            <itm:Type name="JsPanelWithIFrame">
              <itm:JsPanelWithIFrame panelTitle="Employee Contact Details" /> 
            </itm:Type>
            <itm:Container type="ItemGroup" name="HeaderElements">
              <cnt:Container type="InputGroup" name="EmpLookup">
                <cnt:Locator by="css" text="div[class='input-group']"/>                 
                <cnt:Element>
                  <el:Details type="TextOut" name="FormID" />
                  <el:Locator by="id" text="FORM_ID"/>   
                  <el:Function type="element" subtype="TextOut" isDefaultPass="true"/>
                  <el:TestDataIn insertWith="EmployeeLookupByName">                     
                    <din:Text value="Borg"/>
                  </el:TestDataIn>
                  <el:TestDataOut>                     
                    <dout:Text value="0123456M"/>
                  </el:TestDataOut>                        
                </cnt:Element>
              </cnt:Container>
            </itm:Container>
          </menu:MenuItem>
        </menus:Menu>
      </mod:Menus>
    </mods:Module>
